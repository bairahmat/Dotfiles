[global_config]
  suppress_multiple_term_dialog = True
[keybindings]
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
      size = 900, 600
[plugins]
[profiles]
  [[default]]
    background_color = "#263238"
    background_darkness = 0.9
    background_image = None
    font = Monospace 11
    foreground_color = "#ffffff"
    scrollbar_position = hidden
    show_titlebar = False
    use_system_font = False
